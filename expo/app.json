{
  "expo": {
    "name": "Blindly",
    "slug": "blindly",
    "version": "0.5.0",
    "orientation": "portrait",
    "icon": "./assets/main-dark.png",
    "scheme": "blindly",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "buildNumber": "0.3.5"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0e1710",
        "foregroundImage": "./assets/android/res/mipmap-xxxhdpi/ic_launcher_foreground.png",
        "backgroundImage": "./assets/android/res/mipmap-xxxhdpi/ic_launcher_background.png",
        "monochromeImage": "./assets/android/res/mipmap-xxxhdpi/ic_launcher.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "versionCode": 3005
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/web/favicon.ico"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/android/res/mipmap-xxxhdpi/ic_launcher.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-web-browser",
      "expo-secure-store",
      "expo-sqlite",
      "expo-font",
      [
        "expo-video",
        {
          "supportsBackgroundPlayback": true,
          "supportsPictureInPicture": true
        }
      ],
      "expo-audio"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "bda153aa-b6d3-4b06-9c0f-a28402588af1"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/bda153aa-b6d3-4b06-9c0f-a28402588af1"
    }
  }
}
